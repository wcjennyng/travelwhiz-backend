{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/pinNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "review": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1000
      },
      "rating": {
        "type": "string",
        "minLength": 1
      },
      "long": {
        "type": "number"
      },
      "lat": {
        "type": "number"
      },
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "date": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      }

    },
    "additionalProperties": false,
    "required": [
      "title",
      "review",
      "rating",
      "long",
      "lat", 
      "date"
    ]
  }
  